{
  "name": "@nodestrap/spacer",
  "version": "3.0.0",
  "description": "A class library for making a consistent spacing in a html page.",
  "dependencies": {
    "@nodestrap/sass-class-template": "^3.0.2"
  },
  "devDependencies": {
    "tilde-sass": "^1.0.1"
  },
  "scripts": {
    "test": "tilde-sass --file test/*.scss --outFile test/ --mergeSelectors",
    "test-watch": "tilde-sass --file test/*.scss --outFile test/ --mergeSelectors --watch",
    "compile": "tilde-sass --file src/*.scss --outFile dist/ --mergeSelectors --outputStyle compressed",
    "compile-watch": "tilde-sass --file src/*.scss --outFile dist/ --mergeSelectors --outputStyle compressed --watch"
  },
  "cssnano": {
    "preset": [
      "default",
      {
        "calc": false
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodestrap/spacer.git"
  },
  "keywords": [
    "html",
    "space",
    "spacing",
    "spacer",
    "web",
    "page"
  ],
  "author": "nodestrap, heymarco",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nodestrap/spacer/issues"
  },
  "homepage": "https://github.com/nodestrap/spacer#readme"
}
