{
  "name": "@nodestrap/spacer",
  "version": "2.0.0",
  "description": "A class library for making a consistent spacing in a html page.",
  "dependencies": {
    "@nodestrap/sass-class-template": "^2.0.2"
  },
  "devDependencies": {
    "tilde-sass": "^1.0.0"
  },
  "scripts": {
    "test": "tilde-sass --file test/*.scss --outFile test/",
    "compile": "tilde-sass --file src/*.scss --outFile dist/ --outputStyle compressed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodestrap/spacer.git"
  },
  "keywords": [
    "html",
    "space",
    "spacing",
    "spacer",
    "web",
    "page"
  ],
  "author": "nodestrap, heymarco",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nodestrap/spacer/issues"
  },
  "homepage": "https://github.com/nodestrap/spacer#readme"
}
